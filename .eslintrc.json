{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "react-app",
        "airbnb"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2021,
        "sourceType": "module"
    },
    "globals": {
        "chrome": "readonly"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "linebreak-style": "off",
        "semi": "off",
        "react/jsx-no-bind": "off",
        "max-len": "off",
        "react/prop-types": "off",
        "consistent-return": "off",
        "no-plusplus": "off",
        "jsx-a11y/click-events-have-key-events" : "off",
        "jsx-a11y/no-noninteractive-element-interactions": "off"
    }
}
